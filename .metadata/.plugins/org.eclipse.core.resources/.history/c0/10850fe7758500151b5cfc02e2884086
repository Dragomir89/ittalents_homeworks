<?php
// Напишете програма, която създава масив с 10 елемента и
// инициализира всеки от елементите със стойност равна на индекса на
// елемента умножен по 3.
// Да се изведат елементите на екрана.
require_once 'readline.php';


$arr = [];


for ($i = 0; $i <= 10; $i++) {
	$arr[$i] = $i * 3;
}


print_r($arr);
